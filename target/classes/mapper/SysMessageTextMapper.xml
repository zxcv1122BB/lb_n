   <select id="querySysMessageTextByName" parameterType="map"  resultMap="ResultMapWithBLOBs">
	 select 
    <include refid="Base_Column_List" />
    from sys_message_text
    <where>
	     <if test="title != null" >
		   title like CONCAT(CONCAT('%',#{title,jdbcType=VARCHAR}),'%')
	    </if>
	    <if test="delStatus != null and delStatus != '' ">
	    	and del_status = #{delStatus}
	    </if>
    </where>
      ORDER BY create_date DESC
	</select>
	<!-- 查询需要推送的内容 -->
	<select id="selectAllContexts" resultMap="ResultMapWithBLOBs">
	    select smt.* FROM sys_message_text smt WHERE  smt.send_type=#{sendType}
	   <if test="sendType==1">
	       AND FIND_IN_SET((SELECT uvi.vipId FROM user_vip_info uvi,user_info ui WHERE uvi.vipId=ui.VIP_ID AND ui.UID=#{uid}),smt.receiver)
	   </if>
	    <if test="sendType==2">
	       AND FIND_IN_SET(#{uid},smt.receiver)
	   </if>
	       AND smt.id  NOT IN (SELECT  message_id FROM sys_message WHERE user_id=#{uid} AND message_id IN
	       (select smt.id FROM sys_message_text smt WHERE  smt.send_type=#{sendType}  AND FIND_IN_SET(#{uid},smt.receiver)) )
	</select>
